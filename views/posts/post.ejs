<!DOCTYPE html>
<html>
  <head>
    <%- include ../partials/header %>
  </head>
  <body>
    <% posts.forEach(function(el) { %>
    <header>
      <%- include ../partials/nav %>
      <img src="<%= el.img %>" class="post-img" />
    </header>

    <div class="container">
        <h1><%= el.name %></h1>
        <p><%= el.occupation %></p>
        <p><%= el.years %></p>
        <p><%= el.country %></p>
        <p><%= el.bio %></p>

      <% }) %>

    <%- include ../partials/footer %>
  </body>
</html>
