<!DOCTYPE html>
<html>
  <head>
    <%- include ../partials/header %>
  </head>
  <body>
    <%- include ../partials/nav %>

    <div class="content">
    <h1>Individual Posts go Here</h1>
    <% posts.forEach(function(el) { %>
            <p><img src="<%= el.img %>" class="post-img" /></p>
            <p><%= el.name %></p>
            <p><%= el.occupation %></p>
            <p><%= el.years %></p>
            <p><%= el.country %></p>
            <p><%= el.bio %></p>
      <% }) %>
      <div>

    <%- include ../partials/footer %>
  </body>
</html>
